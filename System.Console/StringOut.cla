class StringOut : Out
{
    maide prudate Bool Init()
    {
        base.Init();
        this.InfraInfra : share InfraInfra;
        this.Join : new StringJoin;
        this.Join.Init();
        return true;
    }

    field precate InfraInfra InfraInfra { get { return data; } set { data : value; } }
    field precate StringJoin Join { get { return data; } set { data : value; } }

    maide prudate Bool Write(var String o)
    {
        this.InfraInfra.StringJoinString(this.Join, o);
        return true;
    }

    maide prudate Bool Clear()
    {
        this.Join.Clear();
        return true;
    }

    maide prudate String Result()
    {
        return this.Join.Result();
    }
}